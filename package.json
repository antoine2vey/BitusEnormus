{
  "name": "mappab-to",
  "version": "2.0.0",
  "description": "Mappa bot discord",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/ -d lib/",
    "dev":
      "concurrently \"npm run build -- --watch\" \"nodemon --inspect=0.0.0.0:5858 lib/index.js\"",
    "start": "npm run build && node ./lib/",
    "test": "jest --env=node --forceExit",
    "coverage":
      "jest --coverage --forceExit &&& cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.71.0",
    "flow-typed": "^2.4.0",
    "jest": "^22.4.3",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0",
    "prettier-eslint-cli": "^4.7.0",
    "snekfetch": "^3.6.4"
  },
  "dependencies": {
    "bufferutil": "^3.0.2",
    "cron": "^1.3.0",
    "discord.js": "^11.3.2",
    "discord.js-commando": "^0.10.0",
    "expect": "^22.4.3",
    "ffmpeg": "0.0.4",
    "ffmpeg-binaries": "^3.2.2-3",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "node-opus": "^0.2.9",
    "opusscript": "0.0.6",
    "request": "^2.85.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
