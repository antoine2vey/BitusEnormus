{
  "name": "mappa-bot",
  "version": "2.0.0",
  "description": "Mappa bot discord",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc && node build/index.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll",
    "coverage":
      "jest --coverage --coverageReporters=text-lcov --forceExit | coveralls"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/cron": "^1.3.0",
    "@types/jest": "^23.1.4",
    "@types/mongoose": "^5.0.18",
    "coveralls": "^3.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.2.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.17.5",
    "prettier-eslint-cli": "^4.7.1",
    "snekfetch": "^3.6.4",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "@types/node": "^10.5.1"
  },
  "dependencies": {
    "bufferutil": "^3.0.5",
    "common-tags": "^1.8.0",
    "cron": "^1.3.0",
    "discord.js": "^11.3.2",
    "discord.js-commando": "^0.10.0",
    "expect": "^22.4.3",
    "ffmpeg": "0.0.4",
    "ffmpeg-binaries": "^3.2.2",
    "moment": "^2.22.2",
    "mongoose": "^5.2.0",
    "node-opus": "^0.2.9",
    "opusscript": "0.0.6",
    "request": "^2.85.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"]
  }
}
